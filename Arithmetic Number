int inSequence(int A, int B, int C){
        // code here
        
        int n;
        if(C == 0)
        {
            return A == B;
        }
         
         n = (B-A)/C + 1;
         
         if(n < 1)
         {
             return 0;
         }
         return B == A + (n-1) *C;
    } //13-feb-2023
